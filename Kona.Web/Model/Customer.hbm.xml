<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="Kona"
				   namespace="Kona.Model">
	
	<class name="Customer"
		   table="Customers" >

		<id name="CustomerID">
			<generator class="assigned"/>
		</id>
        <property name="UserName"/>
        <property name="Email"/>
		<property name="First" />
        <property name="Last" />
        <property name="LanguageCode" />

    </class>
</hibernate-mapping>